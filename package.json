{
	"name": "angrezi-archive",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"generate-migrations:db": "drizzle-kit generate --config=db/drizzle.config.ts",
		"push:db": "drizzle-kit push --config=db/drizzle.config.ts",
		"migrate:db": "tsx ./db/migrate.ts",
		"populate-database": "tsx ./db/populateDatabase.ts",
		"start": "pm2 start ecosystem.config.cjs",
		"stop": "pm2 stop ecosystem.config.cjs"
	},
	"devDependencies": {
		"@iconify/svelte": "^4.1.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/bcrypt": "^5.0.2",
		"@types/better-sqlite3": "^7.6.12",
		"@types/eslint": "^9.6.0",
		"@types/formidable": "^3.4.5",
		"@types/jsonwebtoken": "^9.0.8",
		"drizzle-kit": "^0.30.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-organize-imports": "^4.1.0",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-range-slider-pips": "^3.1.2",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.0.3",
		"bcrypt": "^5.1.1",
		"bcryptjs": "^2.4.3",
		"better-sqlite3": "^11.7.0",
		"cookie": "^1.0.2",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.37.0",
		"formidable": "^3.5.2",
		"jsonwebtoken": "^9.0.2",
		"pm2": "^5.4.3",
		"tailwindcss": "^4.0.3",
		"ts-utils": "^6.1.0",
		"tsx": "^4.19.2",
		"utils": "^0.3.1"
	}
}
